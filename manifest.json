{
  "name": "Hello World!",
  "description": "My first Chrome App.",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": ["tts"],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "icons": { "16": "calculator-16.png", "128": "calculator-128.png" }
}
